{
  "packageManager": "yarn@3.2.0",
  "name": "@patman10/kickstart.css",
  "version": "3.1.0",
  "description": "A css file with some utility classes for flex-box, heights, widths, images, icons and more.",
  "repository": "git+https://github.com/PatMan10/kickstart.css.git",
  "keywords": [
    "utility",
    "css",
    "sass",
    "kickstart",
    "PatMan10"
  ],
  "author": "PatMan10",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PatMan10/kickstart.css/issues"
  },
  "homepage": "https://github.com/PatMan10/kickstart.css#readme",
  "devDependencies": {
    "sass": "^1.51.0"
  },
  "scripts": {
    "compile": "yarn sass ./src/lib/kickstart.scss ./dist/kickstart.css",
    "compile:min": "yarn sass --style=compressed  ./src/lib/kickstart.scss ./dist/kickstart.css",
    "watch": "yarn sass --watch ./src/lib:./dist"
  }
}
